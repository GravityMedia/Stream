{
    "name": "gravitymedia/stream",
    "description": "Object oriented stream library for PHP",
    "keywords": [
        "gravitymedia",
        "stream"
    ],
    "homepage": "https://github.com/GravityMedia/Stream",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel SchrÃ¶der",
            "email": "daniel.schroeder@gravitymedia.de",
            "homepage": "https://github.com/pCoLaSD",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "scrutinizer/ocular": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "GravityMedia\\Stream\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GravityMedia\\StreamTest\\": "tests/"
        }
    },
    "scripts": {
        "test": "php vendor/bin/phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "archive": {
        "exclude": [
            "/.*",
            "/composer.*",
            "/phpunit.*",
            "/tests"
        ]
    }
}
