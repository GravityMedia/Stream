{
    "name": "gravitymedia/stream",
    "description": "Stream is an object oriented stream library for PHP",
    "keywords": [
        "gravitymedia",
        "stream"
    ],
    "homepage": "https://github.com/GravityMedia/Stream",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel SchrÃ¶der",
            "email": "daniel.schroeder@gravitymedia.de",
            "homepage": "https://github.com/pCoLaSD",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6"
    },
    "require-dev": {
        "phing/phing": "^2.14",
        "phpunit/phpunit": "^5.3",
        "sami/sami": "^3.2",
        "scrutinizer/ocular": "^1.3",
        "symfony/finder": "^2.8"
    },
    "autoload": {
        "psr-4": {
            "GravityMedia\\Stream\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GravityMedia\\StreamTest\\": "tests/src/"
        }
    },
    "scripts": {
        "test": "php vendor/bin/phing test",
        "doc": "php vendor/bin/phing doc"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "archive": {
        "exclude": [
            "/.*",
            "/*.php",
            "/build.*",
            "/composer.*",
            "/phpunit.*",
            "/tests"
        ]
    }
}
